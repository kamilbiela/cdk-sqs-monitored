{
  "name": "cdk-sqs-monitored",
  "description": "AWS CDK SQS Construct with alarms and dead letter queue",
  "author": {
    "name": "Kamil Biela",
    "email": "kamil.biela@gmail.com",
    "url": "https://kamilbiela.github.io"
  },
  "version": "1.0.3",
  "main": "lib/index.js",
  "license": "MIT",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/kamilbiela/cdk-sqs-monitored.git"
  },
  "scripts": {
    "build": "jsii",
    "build:watch": "jsii -w",
    "package": "jsii-pacmak",
    "test": "jest"
  },
  "jsii": {
    "outdir": "dist",
    "targets": {
      "python": {
        "distName": "cdk-sqs-monitored",
        "module": "cdk_sqs_monitored"
      },
      "java": {
        "package": "com.github.kamilbiela.sqsmonitored",
        "maven": {
          "groupId": "com.github.kamilbiela",
          "artifactId": "cdk-sqs-monitored"
        }
      },
      "dotnet": {
        "namespace": "Kamilbiela.CdkSqsMonitored",
        "packageId": "Kamilbiela.CdkSqsMonitoredPackage",
        "signAssembly": false
      }
    }
  },
  "keywords": [
    "aws",
    "aws-cdk", 
    "cdk", 
    "construct",
    "sqs"
  ],
  "awscdkio": {
    "twitter": "@kamil_biela",
    "announce": true
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.32.2",
    "@types/jest": "^24.0.22",
    "@types/node": "10.17.5",
    "jest": "^24.9.0",
    "jsii": "^1.2.0",
    "jsii-pacmak": "^1.2.0",
    "ts-jest": "^24.1.0",
    "typescript": "~3.7.2"
  },
  "peerDependencies": {
    "@aws-cdk/core": "1.32.2",
    "@aws-cdk/aws-cloudwatch-actions": "1.32.2",
    "@aws-cdk/aws-sns": "1.32.2",
    "@aws-cdk/aws-sns-subscriptions": "1.32.2",
    "@aws-cdk/aws-sqs": "1.32.2",
    "constructs": "2.0.1"
  },
  "dependencies": {
    "constructs": "2.0.1",
    "@aws-cdk/aws-cloudwatch-actions": "1.32.2",
    "@aws-cdk/aws-sns": "1.32.2",
    "@aws-cdk/aws-sns-subscriptions": "1.32.2",
    "@aws-cdk/aws-sqs": "1.32.2",
    "@aws-cdk/core": "1.32.2"
  }
}
